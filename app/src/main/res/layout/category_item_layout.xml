<?xml version="1.0" encoding="utf-8"?>
<com.example.adityaagarwal.room.Views.CategoriesView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/category_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <android.support.v7.widget.CardView
        style="@style/CardPanel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/category_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:textColor="#9726a5e4"
            tools:text="Movies" />
    </android.support.v7.widget.CardView>

</com.example.adityaagarwal.room.Views.CategoriesView>
